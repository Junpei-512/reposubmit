<div class="main submits-show">
  <div class="container">
    <div class="submits-show-item">
      <p>
        <%= @submit.content %>
      </p>
      <div class="submit-time">
        <%= @submit.created_at %>
      </div>
      <div class="submit-menus">
        <%= link_to("編集", "/submits/#{@submit.id}/edit") %>
        <%= link_to("削除", "/submits/#{@submit.id}/destroy", {method: "post"}) %>
      </div>
    </div>
  </div>
</div>